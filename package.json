{
  "name": "@skai/ui",
  "version": "0.1.0",
  "description": "SKAI UI Component Library - Shared React components for the SKAI ecosystem",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "node scripts/build.js",
    "build:css": "tailwindcss -i ./src/styles/index.css -o ./dist/styles.css --minify",
    "build:quiet": "tsup && npm run build:css",
    "build:storybook": "storybook build",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist storybook-static",
    "prepublishOnly": "npm run build:quiet",
    "tokens:sync": "node scripts/sync-figma-tokens.js --from-figma",
    "tokens:generate": "node scripts/sync-figma-tokens.js --generate",
    "tokens:validate": "node scripts/sync-figma-tokens.js --validate"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/manager-api": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.18",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "lucide-react": "^0.344.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "STORYBOOK_TELEMETRY_DISABLED=true storybook dev -p 6006"
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vitest": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SKAI-Trading/skai-ui.git"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "skai",
    "design-system"
  ],
  "author": "SKAI Trading",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SKAI-Trading/skai-ui/issues"
  },
  "homepage": "https://github.com/SKAI-Trading/skai-ui#readme",
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "cmdk": "^1.0.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.2.4",
    "lightweight-charts": "^5.1.0",
    "qrcode.react": "^4.2.0",
    "react-day-picker": "^8.10.1",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^4.5.7",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7"
  }
}
